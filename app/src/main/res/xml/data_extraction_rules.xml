<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <data-extraction-rules>
            <cloud-backup>
                <!-- Customize what data to include or exclude from cloud backup -->
                <include domain="com.example.app" path="/" />
                <exclude domain="com.example.app.cache" />
                <exclude domain="com.example.app.databases" />
            </cloud-backup>
        </data-extraction-rules>

    </cloud-backup>
    <!--
    <device-transfer>
        <include .../>
        <exclude .../>
    </device-transfer>
    -->
</data-extraction-rules>